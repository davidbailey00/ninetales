<!DOCTYPE html>
<html>
  <head>
    <%- type === "full" ? styles : "" %>
  </head>
  <body>
    <div id="app"><%- type === "full" ? app : "" %></div>
    <% if (type === "full") { %>
    <script id="props" type="application/json">
      <%- serialize(data.props, { isJSON: true }) %>
    </script>
    <% bundles.forEach(bundle => { %>
    <script src="/.bundles/<%= bundle %>"></script>
    <% }); %><% } %>
  </body>
</html>
