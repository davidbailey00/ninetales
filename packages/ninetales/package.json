{
  "name": "@ninetales/ninetales",
  "version": "0.1.0",
  "main": "dist/browser/index.js",
  "author": "David Bailey <davidbailey00@outlook.com>",
  "license": "UNLICENSED",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "concurrently 'yarn build:browser' 'yarn build:node'",
    "build:browser": "babel src --out-dir dist/browser --delete-dir-on-start",
    "build:node": "babel src --out-dir dist/node --config-file ./node.babelrc --delete-dir-on-start",
    "verdaccio": "node ../../scripts/verdaccio.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.0",
    "@ninetales/build": "^0.1.0",
    "@ninetales/cli": "^0.1.0",
    "@ninetales/server": "^0.1.0"
  },
  "peerDependencies": {
    "preact": "^10.0.0-rc.3"
  }
}
