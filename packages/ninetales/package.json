{
  "name": "@ninetales/ninetales",
  "version": "0.1.0",
  "main": "dist/browser/index.js",
  "author": "David Bailey <davidbailey00@outlook.com>",
  "license": "UNLICENSED",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "concurrently 'yarn build:browser' 'yarn build:node'",
    "build:browser": "babel src --out-dir dist/browser --presets=@ninetales/babel-preset/browser --delete-dir-on-start --copy-files",
    "build:node": "babel src --out-dir dist/node --presets=@ninetales/babel-preset/node --delete-dir-on-start --copy-files",
    "verdaccio": "node ../../scripts/verdaccio.js",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.0",
    "@ninetales/build": "^0.1.0",
    "@ninetales/cli": "^0.1.0",
    "@ninetales/server": "^0.1.0"
  },
  "peerDependencies": {
    "preact": "^10.0.0-rc.3"
  }
}
